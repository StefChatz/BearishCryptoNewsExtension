{
  "name": "Stef's Bearish news alert",
  "description": "This extension notifys you when bearish news is released in the crypto market",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open Medium Any Authors Stats"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "host_permissions": ["https://cryptopanic.com/"],
  "permissions": ["alarms", "notifications"]
}